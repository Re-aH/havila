{
  "expo": {
    "name": "חבילה עוברת",
    "slug": "havila",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/gifticon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/gift.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffdef"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.reameir.havila",
      "buildNumber": "3",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/gift.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.reameir.havila",
      "versionCode": 8,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/gifticon.png"
    },
    "extra": {
      "eas": {
        "projectId": "78ba3fb8-3a8a-4fb7-9fcf-385a035e0180"
      }
    },
    "owner": "rea_meir",
    "plugins": [
      "expo-audio"
    ]
  }
}
